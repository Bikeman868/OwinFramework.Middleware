<?xml version="1.0"?>
<package >
  <metadata>
    <id>Owin.Framework.OutputCache</id>
    <version>2.8.0</version>
    <title>Owin.Framework.OutputCache</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Owin middleware to cache responses that are expensive to construct and don't change between requests</description>
    <copyright>Copyright Martin Halliday 2015-2019</copyright>
    <releaseNotes>Trace information is less verbose by default but filtering can be configured</releaseNotes>
    <summary>This middleware can be configured to short-circuit certain requests, serving a previously built request rather than hitting the rest of the middleware pipeline to render the response again</summary>
    <projectUrl>https://github.com/Bikeman868/OwinFramework.Middleware/tree/master/OwinFramework.OutputCache</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>OWIN framework output-cache</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Owin.Framework" version="2.8.0" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
      <group targetFramework="net45">
        <dependency id="Owin.Framework" version="2.8.0" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
    </dependencies>
    <references>
      <reference file="OwinFramework.OutputCache.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\OwinFramework.OutputCache\bin\Net40\Release\OwinFramework.OutputCache.dll" target="lib\net40"/>
    <file src="..\OwinFramework.OutputCache\bin\Net45\Release\OwinFramework.OutputCache.dll" target="lib\net45"/>
    <file src="..\OwinFramework.OutputCache\bin\Net40\Release\OwinFramework.OutputCache.dll" target="ref\net40"/>
    <file src="..\OwinFramework.OutputCache\bin\Net45\Release\OwinFramework.OutputCache.dll" target="ref\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>