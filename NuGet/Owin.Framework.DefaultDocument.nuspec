      </group>
      <group targetFramework="net45">
        <dependency id="Owin.Framework" version="2.8.1" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
    </dependencies>
    <references>
      <reference file="OwinFramework.DefaultDocument.dll" />
    </references>
  </metadata>
  <files>
