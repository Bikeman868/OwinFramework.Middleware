    <file src="..\OwinFramework.DefaultDocument\bin\Net45\Release\OwinFramework.DefaultDocument.dll" target="lib\net45"/>
    <file src="..\OwinFramework.DefaultDocument\bin\Net40\Release\OwinFramework.DefaultDocument.dll" target="ref\net40"/>
