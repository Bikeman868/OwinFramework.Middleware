<?xml version="1.0"?>
<package >
  <metadata>
    <id>Owin.Framework.Less</id>
    <version>2.8.0</version>
    <title>Owin.Framework.Less</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Owin middleware to compile LESS files to CSS files on the fly</description>
    <copyright>Copyright Martin Halliday 2014-2019</copyright>
    <releaseNotes>Trace information is less verbose by default but filtering can be configured</releaseNotes>
    <summary>Compiles LESS files to CSS files on the fly</summary>
    <projectUrl>https://github.com/Bikeman868/OwinFramework.Middleware/tree/master/OwinFramework.Less</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>OWIN framework LESS compiler middleware</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Owin.Framework" version="2.8.0" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
        <dependency id="DotlessClientOnly" version="1.5.2" />
      </group>
      <group targetFramework="net45">
        <dependency id="Owin.Framework" version="2.8.0" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
        <dependency id="DotlessClientOnly" version="1.5.2" />
      </group>
    </dependencies>
    <references>
      <reference file="OwinFramework.Less.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\OwinFramework.Less\bin\Net40\Release\OwinFramework.Less.dll" target="lib\net40"/>
    <file src="..\OwinFramework.Less\bin\Net45\Release\OwinFramework.Less.dll" target="lib\net45"/>
    <file src="..\OwinFramework.Less\bin\Net40\Release\OwinFramework.Less.dll" target="ref\net40"/>
    <file src="..\OwinFramework.Less\bin\Net45\Release\OwinFramework.Less.dll" target="ref\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>