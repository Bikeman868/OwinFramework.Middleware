<?xml version="1.0"?>
<package >
  <metadata>
    <id>Owin.Framework.Documenter</id>
    <version>2.7.1</version>
    <title>Owin.Framework.Documenter</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Owin middleware to document other Owin middleware</description>
    <copyright>Copyright Martin Halliday 2014-2019</copyright>
    <releaseNotes>Trace information is less verbose by default but filtering can be configured</releaseNotes>
    <summary>Generates documentation of all endpoints in a web application.</summary>
    <projectUrl>https://github.com/Bikeman868/OwinFramework.Middleware/tree/master/OwinFramework.Documenter</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>OWIN framework documentation</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Owin.Framework" version="2.7.1" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
      <group targetFramework="net45">
        <dependency id="Owin.Framework" version="2.7.1" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
    </dependencies>
    <references>
      <reference file="OwinFramework.Documenter.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\OwinFramework.Documenter\bin\Net40\Release\OwinFramework.Documenter.dll" target="lib\net40"/>
    <file src="..\OwinFramework.Documenter\bin\Net45\Release\OwinFramework.Documenter.dll" target="lib\net45"/>
    <file src="..\OwinFramework.Documenter\bin\Net40\Release\OwinFramework.Documenter.dll" target="ref\net40"/>
    <file src="..\OwinFramework.Documenter\bin\Net45\Release\OwinFramework.Documenter.dll" target="ref\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>