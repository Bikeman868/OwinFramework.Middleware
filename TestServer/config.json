{
    "middleware": {
        "visualizer": {
            "path": "/owin/pipeline",
            "enabled": true,
            "requiredPermission": "administrator"
        },
        "analysis": {
            "path": "/owin/analytics",
            "enabled": true,
            "requiredPermission": "",
            "defaultFormat": "application/xml"
        },
        "documenter": {
            "path": "/owin/endpoints",
            "enabled": true,
            "requiredPermission": ""
        },
        "exceptions": {
            "message": "<p>This is a <i>custom</i> message.</p>",
            "template": "exception.html",
            "localhost": true,
            "emailAddress": "bitbucket@mailinator.com",
            "subject": "Test server unhandled exception"
        },
        "staticFiles": {
            "staticFilesRootUrl": "/",
            "documentationRootUrl": "/owin/staticFiles",
            "rootDirectory": "~\\",
            "enabled": true,
            "includeSubFolders": true,
            "fileExtensions": [
                { "extension": ".html", "mimeType": "text/html" },
                { "extension": ".ico", "mimeType": "image/ico" },
                { "extension": ".js", "mimeType": "application/javascript" },
                { "extension": ".dart", "mimeType": "application/dart" }
            ],
            "maximumFileSizeToCache": 10000,
            "totalCacheSize": 1000000,
            "maximumCacheTime": "00:30:00",
            "requiredPermission": ""
        },
        "less":  {
            "rootUrl":  "/",
            "documentationRootUrl": "/owin/less",
            "rootDirectory": "~\\",
            "enabled": true
        },
        "defaultDocument": {
            "defaultPage": "/index.html",
            "documentationRootUrl": "/owin/defaultDocument"
        },
        "dart": {
            "defaultDocument": "index.html",
            "documentationRootUrl": "/owin/dart",
            "uiRootUrl": "/ui",
            "dartUiRootUrl": "/ui/web",
            "compiledUiRootUrl": "/ui/build/web",
        },
        "outputCache": { 
            "maximumCacheTime": "01:00:00"
        },
        "versioning": { 
            "version": 1,
            "documentationRootUrl": "/owin/versioning",
            "mimeTypes": [
                "text/html", 
                "text/css", 
                "application/javascript",
                "application/dart"
            ],
            "fileExtensions": [
                ".css", 
                ".html",
                ".ico",
                ".js",
                ".dart"
            ],
            "browserCacheTime": "90",
            "exactVersion": "true"
        }
    }
}
